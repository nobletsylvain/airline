; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Airline Tycoon Prototype"
config/description="A prototype airline tycoon game based on airline-club.com"
run/main_scene="res://scenes/MainMenu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DataLoader="*res://scripts/DataLoader.gd"
GameData="*res://scripts/GameData.gd"
UISoundManager="*res://scripts/audio/UISoundManager.gd"
UIAnimations="*res://scripts/ui/UIAnimations.gd"
AmbientAudioManager="*res://scripts/audio/AmbientAudioManager.gd"
GDAIMCPRuntime="*res://addons/gdai-mcp-plugin-godot/gdai_mcp_runtime.gd"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Airline Tycoon Prototype"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdai-mcp-plugin-godot/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
